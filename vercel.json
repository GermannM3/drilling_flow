{
  "version": 2,
  "builds": [
    {
      "src": "app/vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/app/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app/vercel.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  }
} 