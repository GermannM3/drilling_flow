{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11",
    "PYTHONPATH": ".",
    "APP_MODULE": "app.main:app",
    "LOG_LEVEL": "INFO",
    "TESTING": "false"
  }
} 