{
  "version": 2,
  "buildCommand": "bash vercel-build.sh",
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/python/index.js"
    }
  ],
  "env": {
    "DISABLE_BOT": "False",
    "USE_POLLING": "True",
    "PYTHONPATH": "/var/task"
  },
  "builds": [
    {
      "src": "api/python/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ]
} 